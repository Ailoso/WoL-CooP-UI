            <Texture val="Assets\Textures\sc2_ui_newplayerxp_hexagon_shimmermask.dds" layer="1"/>
            <BlendMode val="Add" layer="1"/>

            <Animation name="ColoredShimmer">
                <Event event="OnShown" action="Play" frame="$this"/>

                <Controller type="Property" property="TextureCoordRotation" curve="True" index="1"  end="Loop" frame="$this">
                    <Key type="Property" time="0.0" value="0" inOut="Linear"/>
                    <Key type="Property" time="3.5" value="360" inOut="Linear"/>
                </Controller>
            </Animation>
        </Frame>
