				<Event event="OnShown" action="DirectionForward" frame="$parent"/>
				<Event event="OnShown" action="Reset,Play" frame="$parent"/>
					 
				<Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Pause" >
					<Key type="Curve" time="0.0" value="1" out="Fast"/>
					<Key type="Curve" time="0.3" value="-1" in="Slow"/>
