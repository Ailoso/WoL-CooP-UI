        <Animation name="Show">
            <Event event="OnShown" action="Reset,Play" frame="$this"/>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="False" unclip="False" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time="1.5" value="434" in="Slow"/>
            </Controller>
        </Animation>

        <Animation name="Hide">
            <Event event="Hide" action="Reset,Play" frame="$this"/>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255" out="Fast"/>
                <Key type="Curve" time=".8" value="0" in="Slow"/>
            </Controller>

            <Controller type="Visibility" end="Pause">
                <Key type="Bool" time=".8" value="False"/>
            </Controller>
        </Animation>
    </Frame>

