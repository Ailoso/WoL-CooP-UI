                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.2" value="255"/>
            </Controller>

            <Controller type="Dimension" end="Pause" frame="$this/ResearchGlow" dimension="Width" relative="False">
                <Key type="Curve" time="0.0" value="210" inout="Fast"/>
                <Key type="Curve" time="0.7" value="128" inout="Slow"/>
            </Controller>

            <Controller type="Dimension" end="Pause" frame="$this/ResearchGlow" dimension="Height" relative="False">
                <Key type="Curve" time="0.0" value="210" inout="Fast"/>
                <Key type="Curve" time="0.7" value="128" inout="Slow"/>
            </Controller>

            <!-- Hit -->
            <Controller type="Visibility" end="Pause" frame="$this/GlowHit">
                <Key type="Bool" time="0.3" value="True"/>
            </Controller>

            <Controller type="Fade" end="Pause" frame="$this/GlowHit">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.3" value="0"/>
                <Key type="Curve" time="0.4" value="255"/>
                <Key type="Curve" time="0.7" value="255"/>
                <Key type="Curve" time="0.8" value="0"/>
            </Controller>

            <Controller type="Dimension" end="Pause" frame="$this/GlowHit" dimension="Width" relative="False">
                <Key type="Curve" time="0.0" value="100" inout="Fast"/>
                <Key type="Curve" time="0.3" value="100" inout="Fast"/>
                <Key type="Curve" time="0.8" value="130" inout="Slow"/>
            </Controller>

            <Controller type="Dimension" end="Pause" frame="$this/GlowHit" dimension="Height" relative="False">
                <Key type="Curve" time="0.0" value="100" inout="Fast"/>
                <Key type="Curve" time="0.3" value="100" inout="Fast"/>
                <Key type="Curve" time="0.8" value="130" inout="Slow"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="$this/IconShimmer">
                <Key type="Bool" time="0.3" value="True"/>
            </Controller>

            <Controller type="Sound" end="Pause" frame="$this">
                <Key type="Sound" time="0.0" sound="@UI_ResearchPanel_ConfirmationProtoss"/>
            </Controller>
