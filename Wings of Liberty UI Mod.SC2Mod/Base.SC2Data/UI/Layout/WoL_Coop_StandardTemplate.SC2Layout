            <Width val="90"/>
            <Height val="90"/>

            <Animation name="Display">
                <Event event="OnShown" action="Play" frame="$this"/>

                <Controller type="Dimension" end="Pause" frame="$this" dimension="Height" relative="True">
                    <Key type="Curve" time="0.0" value="10"/>
                    <Key type="Curve" time="0.4" value="0"/>
                </Controller>

                <Controller type="Dimension" end="Pause" frame="$this" dimension="Width" relative="True">
                    <Key type="Curve" time="0.0" value="10"/>
                    <Key type="Curve" time="0.4" value="0"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="0"/>
                    <Key type="Curve" time="0.2" value="255"/>
                </Controller>

                <Controller type="Fade" end="Pause" frame="$parent/IconMask">
                    <Key type="Curve" time="0.0" value="0"/>
                    <Key type="Curve" time="0.2" value="150"/>
                </Controller>
            </Animation>
        </Frame>

        <Frame type="Image" name="Window">
