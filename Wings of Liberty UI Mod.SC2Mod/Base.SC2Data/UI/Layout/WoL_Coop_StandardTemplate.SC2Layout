    <Frame type="Button" name="TechButton">
        <NormalImage val="NormalImage"/>
        <HoverImage val="HoverImage"/>
        <HitTestFrame val="HitTestFrame"/>
        <ClickSound val="@UI_ActionButtonSelect"/>
		<Toggleable val="True"/>

        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="Assets\Textures\ui_techpurchase_techbutton_available_normalpressed.dds"/>
            <TextureType val="Border"/>
            <Tiled val="false"/>
            <StateCount val="2"/>
            <DisabledAlpha val="128"/>
            <ColorAdjustMode val="Fill"/>
            <AdjustmentColor val="73,216,209"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Texture val="Assets\Textures\ui_techpurchase_techbutton_available_normaloverpressedover.dds"/>
            <TextureType val="Border"/>
            <Tiled val="false"/>
            <StateCount val="2"/>
            <DisabledAlpha val="128"/>
            <ColorAdjustMode val="Fill"/>
            <AdjustmentColor val="73,216,209"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
		
        <StateGroup name="ToggleState">
            
            <State name="Normal">
                <When type="Property" frame="$this" Toggled="false"/>
            </State>

            <State name="Over">
                <When type="Property" frame="$this" Toggled="true"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="0,255,0"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="0,255,0"/>
            </State>
        </StateGroup>
		
        <StateGroup name="TechPurchaseState">
            <DefaultState val="Available"/>
            
            <State name="Available">
                <When type="AnimationState" PurchaseState="Available"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="0,254,254"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="0,254,254"/>
                <Action type="SetProperty" frame="$this/Icon" AdjustmentColor="0,254,254"/>
            </State>
			
            <State name="Hired">
                <When type="AnimationState" PurchaseState="Hired"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="49,71,70"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="49,71,70"/>
				<Action type="SetProperty" frame="$this/Icon" AdjustmentColor="49,71,70"/>
            </State>
			
            <State name="NoCredit">
                <When type="AnimationState" PurchaseState="NoCredit"/>
                <Action type="SetProperty" frame="$this/HoverImage" AdjustmentColor="170,49,49"/>
                <Action type="SetProperty" frame="$this/NormalImage" AdjustmentColor="170,49,49"/>
				<Action type="SetProperty" frame="$this/Icon" AdjustmentColor="170,49,49"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="Image" name="TechUnitPanel">
		<Texture val="Assets\Textures\ui_techpurchase_unitbutton_availableselected.dds"/>
		<TextureType val="Normal"/>
		<Tiled val="false"/>
		<StateCount val="2"/>
		<StateIndex val="0"/>
		<DesaturateOnDisable val="false"/>
		<DisabledAlpha val="128"/>
		<Height val="360"/>
		<Width val="250"/>
		<CollapseLayout val="True"/>
		<ColorAdjustMode val="Colorize"/>
		
        <StateGroup name="SelectionState">
            
            <State name="Normal">
                <When type="Property" frame="$this/TechButton1" Toggled="false"/>
                <When type="Property" frame="$this/TechButton2" Toggled="false"/>

                <Action type="SetProperty" frame="$this" StateIndex="0"/>
            </State>
			
            <State name="Tech1InUse">
                <When type="Property" frame="$this/TechButton1" Toggled="true"/>
                <When type="Property" frame="$this/TechButton2" Toggled="false"/>

                <Action type="SetProperty" frame="$this" StateIndex="1"/>
				<Action type="SetProperty" frame="$this" AdjustmentColor="0,255,0"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitSelected"/>
            </State>
			
            <State name="Tech2InUse">
                <When type="Property" frame="$this/TechButton1" Toggled="false"/>
                <When type="Property" frame="$this/TechButton2" Toggled="true"/>

                <Action type="SetProperty" frame="$this" StateIndex="1"/>
				<Action type="SetProperty" frame="$this" AdjustmentColor="0,255,0"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitSelected"/>
            </State>
        </StateGroup>
		
        <StateGroup name="PurchaseState">
            <DefaultState val="Available"/>
            
            <State name="Available">
                <When type="AnimationState" PurchaseState="Available"/>
                <Action type="SetProperty" frame="$this/UnitIcon" Visible="False"/>
                <Action type="SetProperty" frame="$this/UnitModel" Visible="True"/>
				<Action type="SetProperty" frame="$this" AdjustmentColor="0,254,254"/>
            </State>
			
            <State name="Hired">
                <When type="AnimationState" PurchaseState="Hired"/>
                <Action type="SetProperty" frame="$this/UnitIcon" Visible="True"/>
                <Action type="SetProperty" frame="$this/UnitModel" Visible="False"/>
				<Action type="SetProperty" frame="$this" AdjustmentColor="49,71,70"/>
				<Action type="SetProperty" frame="$this/UnitIcon" AdjustmentColor="49,71,70"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitPurchased"/>
            </State>
			
            <State name="NoCredit">
                <When type="AnimationState" PurchaseState="NoCredit"/>
                <Action type="SetProperty" frame="$this/UnitIcon" Visible="True"/>
                <Action type="SetProperty" frame="$this/UnitModel" Visible="False"/>
				<Action type="SetProperty" frame="$this" AdjustmentColor="170,49,49"/>
				<Action type="SetProperty" frame="$this/UnitIcon" AdjustmentColor="170,49,49"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitNoCredit"/>
            </State>
			
            <State name="NewTech">
                <When type="AnimationState" PurchaseState="NewTech"/>
                <Action type="SetProperty" frame="$this/UnitIcon" Visible="False"/>
                <Action type="SetProperty" frame="$this/UnitModel" Visible="True"/>
				<Action type="SetProperty" frame="$this" Texture="Assets\Textures\ui_techpurchase_unitbutton_normaloverpressedover.dds"/>
				<Action type="SetProperty" frame="$this" AdjustmentColor="0,254,254"/>
				<Action type="SetProperty" frame="$this/UnitLabel" Style="WolCoopTechUnitNewTech"/>
            </State>
        </StateGroup>
		
		<Frame type="Label" name="UnitLabel">
			<Anchor side="Top" relative="$parent" pos="Min" offset="31"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="22"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="-22"/>
			<Style val="WolCoopTechUnitAvailable"/>
		</Frame>
		
		<Frame type="Image" name="UnitIcon">
			<Anchor side="Top" relative="$parent" pos="Min" offset="60"/>
			<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
