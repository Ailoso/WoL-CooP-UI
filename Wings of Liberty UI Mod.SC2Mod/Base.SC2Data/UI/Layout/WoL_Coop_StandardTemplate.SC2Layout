        <StateGroup name="ResearchState">
            <DefaultState val="Zerg"/>
        
            <State name="Disabled">
                <When type="AnimationState" frame="$this" operator="Equal" ResearchState="Disabled"/>

                <Action type="SetProperty" frame="$this" Enabled="False"/>
            </State>

            <State name="Enabled">
                <When type="AnimationState" frame="$this" operator="Equal" ResearchState="Enabled"/>
    
                <Action type="SetProperty" frame="$this" Enabled="True"/>
            </State>
        
            <State name="Protoss">
                <When type="AnimationState" frame="$this" operator="Equal" ResearchState="Protoss"/>

                <Action type="SetProperty" frame="$this/NormalImage" Texture="Assets\Textures\ui_button_confirmation_normalpressed_protoss.dds"/>
                <Action type="SetProperty" frame="$this/HoverImage" Texture="Assets\Textures\ui_button_confirmation_normaloverpressedover_protoss.dds"/>
            </State>
        
            <State name="Zerg">
                <When type="AnimationState" frame="$this" operator="Equal" ResearchState="Zerg"/>

                <Action type="SetProperty" frame="$this/NormalImage" Texture="Assets\Textures\ui_button_confirmation_normalpressed_zerg.dds"/>
                <Action type="SetProperty" frame="$this/HoverImage" Texture="Assets\Textures\ui_button_confirmation_normaloverpressedover_zerg.dds"/>
