            <AlphaLayer val="True" layer="0"/>
            
            <Texture val="Assets\Textures\ui_techpurchase_upgradefanfare_upgradefanfare_scanlines.dds" layer="1"/>
            <TextureCoords top="0" left="1" bottom="1" right="0" layer="1"/>
            <BlendMode val="Add" layer="1"/>
            <WrapUV val="True" layer="1"/>

            <Animation name="ShimmerAnim">
                <Event event="OnShown" action="DirectionForward" frame="$this"/>
                <Event event="OnShown" action="Play" frame="$this"/>
                <Event event="OnHidden" action="Reset,Stop" frame="$this"/>

                <Controller type="Fade" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="0"/>
                    <Key type="Curve" time="0.1" value="200"/>
                </Controller>
            
                <Controller type="LayerUV" layer="1" side="Left" relative="False" sidelock="True" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="0.8" value="1" in="linear"/>
                </Controller>

                <Controller type="LayerUV" layer="1" side="Bottom" relative="False" sidelock="True" end="Loop" >
                    <Key type="Curve" time="0" value="-1" out="linear"/>
                    <Key type="Curve" time="0.8" value="1" in="linear"/>
                </Controller>
            </Animation>

            <Animation name="ScanlineStop">
                <Event event="StopScanlines" action="Play" frame="$this"/>

                <Controller type="Fade" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="200"/>
                    <Key type="Curve" time="0.1" value="0"/>
                </Controller>

                <Controller type="Visibility" end="Pause" frame="$this">
                    <Key type="Bool" time="0.1" value="False"/>
                </Controller>
            </Animation>
        </Frame>

