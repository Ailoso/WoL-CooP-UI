    <Frame type="Frame" name="TeammatePanelTemplate">

        <Width val="120"/>
        <Height val="120"/>
        <Visible val="False"/>
        <CollapseLayout val="True"/>

        <Animation name="Shown">
            <Event event="OnShown" action="Play" frame="$this"/>

            <Controller type="Anchor" end="Pause" frame="$this" side="Right" relative="False">
                <Key type="Curve" time="0.0" value="110" inout="Fast"/>
                <Key type="Curve" time="0.5" value="0" inout="Slow"/>
            </Controller>

            <Controller type="Fade" end="Pause" frame="$this">
                <Key type="Curve" time="0.0" value="0"/>
                <Key type="Curve" time="0.2" value="255"/>
            </Controller>

            <Controller type="Visibility" end="Pause" frame="$this/CheckMark">
                <Key type="Bool" time="0.0" value="False"/>
                <Key type="Bool" time="0.6" value="True"/>
            </Controller>
        </Animation>

        <StateGroup name="Player">
            <DefaultState val="1"/>
            
            <State name="1">
                <When type="AnimationState" frame="$this" operator="Equal" Player="1"/>

                <Action type="SetProperty" frame="$this/BorderShimmer" AdjustmentColor="#Player1Color"/>
            </State>
            
            <State name="2">
                <When type="AnimationState" frame="$this" operator="Equal" Player="2"/>

                <Action type="SetProperty" frame="$this/BorderShimmer" AdjustmentColor="#Player2Color"/>
            </State>
            
            <State name="3">
                <When type="AnimationState" frame="$this" operator="Equal" Player="3"/>

                <Action type="SetProperty" frame="$this/BorderShimmer" AdjustmentColor="#Player3Color"/>
            </State>
        </StateGroup>

        <Frame type="Image" name="Border">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
