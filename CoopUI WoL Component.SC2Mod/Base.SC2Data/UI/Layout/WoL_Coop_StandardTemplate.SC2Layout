    <Frame type="Image" name="TossResearchAvailable">
        <Texture val="Assets\Textures\ui_research_frame_available_line_protoss.dds"/>
        <TextureType val="Border"/>
<!--

        <Texture val="Assets\Textures\ui_research_frame_available_box.dds"/>

        <Frame type="Image" name="Border">
