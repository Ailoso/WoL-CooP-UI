            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>

        <Animation name="Available">
            <Event event="Available" action="Reset,Play"/>
                        
            <Controller type="Texture" end="Pause" frame="$this/NormalImage">
               <Key type="Image" time="0" image="Assets\Textures\ui_techpurchase_techbutton_available_normalpressed.dds"/>
            </Controller>

            <Controller type="Texture" end="Pause" frame="$this/HoverImage">
               <Key type="Image" time="0" image="Assets\Textures\ui_techpurchase_techbutton_available_normaloverpressedover.dds"/>
            </Controller>
        </Animation>

        <Animation name="Hired">
            <Event event="Hired" action="Reset,Play"/>
            <Event event="Normal" action="Reset,Stop"/>
                        
            <Controller type="Property" property="AdjustmentColor" end="Pause" frame="$this/NormalImage">
                <Key type="Property" time="0" value="49,71,70"/>
            </Controller>

            <Controller type="Property" property="AdjustmentColor" end="Pause" frame="$this/HoverImage">
                <Key type="Property" time="0" value="49,71,70"/>
            </Controller>

            <Controller type="Property" property="AdjustmentColor" end="Pause" frame="$this/Icon">
                <Key type="Property" time="0" value="49,71,70"/>
            </Controller>
        </Animation>

        <Animation name="Selected">
            <Event event="Selected" action="Reset,Play"/>
            <Event event="Normal" action="Reset,Stop"/>

            <Controller type="Property" property="StateIndex" end="Pause" frame="$this">
                <Key type="Property" time="0" value="1"/>
            </Controller>
                        
            <Controller type="Property" property="AdjustmentColor" end="Pause" frame="$this/NormalImage">
                <Key type="Property" time="0" value="0,255,0"/>
            </Controller>

            <Controller type="Property" property="AdjustmentColor" end="Pause" frame="$this/HoverImage">
                <Key type="Property" time="0" value="0,255,0"/>
            </Controller>
        </Animation>

    </Frame>

